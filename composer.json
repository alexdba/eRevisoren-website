{
    "name": "erevisoren",
    "description": "eRevisoren.dk Website",
    "keywords": ["yii2", "framework", "eRevisoren"],
    "homepage": "http://www.erevisoren.dk",
    "type": "project",
  	"license": "Proprietary",
      "authors": [
          {
             "name": "David Andersen",
             "email": "mrdavidandersen@gmail.com",
              "homepage": "http://www.erevisoren.dk/"
          }
      ],
  	"support": {
  		"issues": "http://www.erevisoren.dk/",
  		"source": "http://www.erevisoren.dk/"
  	},
    "minimum-stability": "dev",
    "require": {
      "php": ">=5.4.0",
      "yiisoft/yii2": ">=2.0.1",
      "yiisoft/yii2-bootstrap": "*",
      "yiisoft/yii2-swiftmailer": "*",
      "jasny/bootstrap": ">=3.1.3",
      "kartik-v/yii2-widgets": "*",
      "kartik-v/yii2-password": "dev-master",
      "kartik-v/yii2-datecontrol": "dev-master",
      "2amigos/yii2-file-input-widget" : "*",
      "2amigos/yii2-multi-select-widget" : "*",
      "tecnick.com/tcpdf": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}

